#include <stdio.h>
#include <time.h>

extern "C" int init (int d_lev);
extern "C" int connect_to_server ( String server_addr[], int port);
extern "C" int send_to_server (char message[]);
extern "C" int recieve_from_server (char message[]);

int main(){

init(1);
connect_to_server("192.168.1.2", 1024);
send_to_server("Hello server");
char message[24];
message = recieve_from_server();
printf("%s", message);

return 0;
}
